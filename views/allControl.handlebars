<div class="container-contact100" style='width:85%;float:right;margin:0px;padding:3px;'>
    <div class="wrap-contact100" style='padding-left:5px;width:98%;'>
        <form class="contact100-form validate-form">
            <span class="contact100-form-title">
                数据直报系统
            </span>

            <div class="wrap-input100 validate-input bg1" data-validate="请输入商家公司名称">
                <span class="label-input100">商家公司名称 *</span>
                <input class="input100" type="text" name="name" placeholder="请输入商家公司名称">
            </div>



            <div class="wrap-input100 validate-input bg1 rs1-wrap-input100" data-validate="请输入主营类目">
                <span class="label-input100">主营类目 *</span>
                <input class="input100" type="text" name="type" placeholder="请输入主营类目">
            </div>

            <div class="wrap-input100 validate-input bg1 rs1-wrap-input100" data-validate="请输入日均单量">
                <span class="label-input100">日均单量</span>
                <input class="input100" type="text" name="number" placeholder="请输入日均单量">
            </div>


            <div class="wrap-input100 validate-input bg1 rs1-wrap-input100" data-validate="请输入口岸/城市">
                <span class="label-input100">口岸/城市 *</span>
                <input class="input100" type="text" name="city" placeholder="请输入口岸/城市">
            </div>

            <div class="wrap-input100 validate-input bg1 rs1-wrap-input100" data-validate="请输入贸易类型">
                <span class="label-input100">贸易类型 *</span>
                <input class="input100" type="text" name="type" placeholder="请输入贸易类型">
            </div>

            <div class="wrap-input100 validate-input bg1 rs1-wrap-input100" data-validate="请输入序号">
                <span class="label-input100">手机号 </span>
                <input class="input100" type="text" name="phone" placeholder="请输入序号">
            </div>

            <div class="wrap-input100 validate-input bg1 rs1-wrap-input100" data-validate="请输入客户等级划分">
                <span class="label-input100">客户等级划分 *</span>
                <input class="input100" type="text" name="level" placeholder="请输入客户等级划分">
            </div>



            <div class="wrap-input100 validate-input bg0 rs1-alert-validate">
                <span class="label-input100">备注</span>
                <textarea class="input100" name="message" placeholder="请输入备注信息"></textarea>
            </div>

            <div class="container-contact100-form-btn">
                <button class="contact100-form-btn" id='submitButton' data-toggle="modal" data-target="#myModal">
                    <span>
                        提交
                        <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
                    </span>
                </button>
            </div>

            <!-- Modal -->
            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title" id="myModalLabel">数据直报</h4>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            提交成功！
                        </div>
                        <div class="modal-footer" style='float: left;'>
                            <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>

